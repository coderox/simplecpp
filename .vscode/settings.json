// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        // files
        "**/*.exe": true,
        "**/*.obj": true,
        "**/*.pdb": true,
        "**/*.winmd": true,
        "**/*.db": true,
        "**/*.opendb": true,
        "**/*.out": true,
        "**/*.ilk": true,

        // directories
        "**/x64": true,
        "**/x86": true,
        "**/Debug": true,
        "**/ipch": true,
        "**/.vs": true
    }    
}